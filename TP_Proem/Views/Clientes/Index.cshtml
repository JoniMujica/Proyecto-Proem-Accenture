@model List<Cliente>

<h1>Clientes</h1>

@if(Model.Count > 0)
{
    <table>
        <thead>
            <tr>
                <th>Codigo</th>
                <th>Nombre</th>
                <th>Direccion</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Cliente in Model)
            {
                <tr>
                    <td>@Cliente.Codigo</td>
                    <td>@Cliente.Nombre</td>
                    <td>@Cliente.Direccion</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No hay datos para mostrar</p>
}
